{
	"name": "<%= props.projectName %>",
	"version": "<%= props.projectVersion %>",
	"description": "<%= props.projectDesc %>",
	"scripts": {
		"start": "node ./dist/app/server.js",
		"build": "npm run compile && npm run test",
		"clean": "rimraf dist coverage .nyc_output",
		"dedup": "rimraf node_modules/**/node_modules/**/typings",
		"compile": "npm run clean && npm run tslint && tsc --pretty",
		"release": "npm run build && npm run definition",
		"test": "nyc --reporter=json --reporter=html --include dist/app/**/*.js mocha --recursive dist/test",
		"tslint": "tslint -c tslint.json 'src/**/*.ts'",
		"watch": "npm run clean && npm run tslint && tsc --watch --pretty"
	},
	"dependencies": {
		"@micro-fleet/cache": "~1.1",
		"@micro-fleet/common": "~1.2",
		"@micro-fleet/id-generator": "~1.0",
		"@micro-fleet/microservice": "~1.2",
		"@micro-fleet/persistence": "~2.0",
		"@micro-fleet/web": "~1.0",
		"scrypt": "~6.0"
	},
	"devDependencies": {
		"@types/chai": "~4.1",
		"@types/chai-spies": "~0.0.1",
		"@types/express": "~4.0",
		"@types/joi": "~13.0",
		"@types/knex": "~0.14",
		"@types/mocha": "~5.2",
		"@types/node": "~8.10",
		"mocha": "~5.2",
		"nyc": "~12.0",
		"rimraf": "~2.6",
		"tslint": "~5.10",
		"typescript": "~2.9"
	},
	"engines": {
		"node": ">=8.0.0"
	}
}
